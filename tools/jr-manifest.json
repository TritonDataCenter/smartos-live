{
    "jrVersion": 1,
    "description": "SmartOS platform public repositories",
    "repoCandidateSearch": {
        "description": "public github.com/joyent repos directly relevant to development of SmartOS",
        "type": "public",
        "includeArchived": false
    },
    "blessedLabels": [
        {
            "name": "meta",
            "type": "boolean",
            "description": "a repo that isn't code, but is relevant for product development"
        },
        {
            "name": "release",
            "type": "boolean",
            "description": "a top-level repo for a Manta release component"
        }
    ],
    "defaults": {
        "labels": {
            "smartos": true,
            "public": true
        }
    },
