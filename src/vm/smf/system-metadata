set -o xtrace

. /lib/svc/share/smf_include.sh

PATH=/usr/sbin:/usr/bin; export PATH

case "$1" in
'start')
    /usr/bin/ctrun -l child -o noorphan /usr/vm/sbin/metadata 2>&1 &
    ;;

'stop')
    ;;

*)
    echo "Usage: $0 { start | stop }"
    exit $SMF_EXIT_ERR_FATAL
    ;;
esac
exit $SMF_EXIT_OK
